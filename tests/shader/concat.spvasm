                     OpCapability VulkanMemoryModel
                     OpCapability Shader
                     OpCapability Int8
                     OpCapability GraphARM
                     OpCapability TensorsARM
                     OpExtension "SPV_ARM_graph"
                     OpExtension "SPV_ARM_tensors"
             %tosa = OpExtInstImport "TOSA.001000.1"
                     OpMemoryModel Logical Vulkan
                     OpDecorate %input_tensor_ptr0 DescriptorSet 0
                     OpDecorate %input_tensor_ptr0 Binding 0
                     OpDecorate %input_tensor_ptr1 DescriptorSet 0
                     OpDecorate %input_tensor_ptr1 Binding 1
                     OpDecorate %output_tensor_ptr DescriptorSet 0
                     OpDecorate %output_tensor_ptr Binding 2
             %uint = OpTypeInt 32 0
           %uint_0 = OpConstant %uint 0
           %uint_1 = OpConstant %uint 1
           %uint_4 = OpConstant %uint 4
             %int8 = OpTypeInt 8 0
       %int8tensor = OpTypeTensorARM %int8 %uint_4
%ptr_UniformConstant_int8tensor = OpTypePointer UniformConstant %int8tensor
%graph_1I_T4I8_T4I8 = OpTypeGraphARM 2 %int8tensor %int8tensor %int8tensor
%input_tensor_ptr0 = OpVariable %ptr_UniformConstant_int8tensor UniformConstant
%input_tensor_ptr1 = OpVariable %ptr_UniformConstant_int8tensor UniformConstant
%output_tensor_ptr = OpVariable %ptr_UniformConstant_int8tensor UniformConstant
                     OpGraphEntryPointARM %graph "concat" %input_tensor_ptr0 %input_tensor_ptr1 %output_tensor_ptr
            %graph = OpGraphARM %graph_1I_T4I8_T4I8
              %in0 = OpGraphInputARM %int8tensor %uint_0
              %in1 = OpGraphInputARM %int8tensor %uint_1
           %concat = OpExtInst %int8tensor %tosa CONCAT %uint_1 %in0 %in1
                     OpGraphSetOutputARM %concat %uint_0
                     OpGraphEndARM
